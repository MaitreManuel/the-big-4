{
    "name": "the-big-4",
    "author": "Trystan Eveno",
    "licence": "MIT",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "lint": "eslint src/**",
        "lint:fix": "eslint --fix src/**",
        "test": "npm run lint",
        "install": "npm run build:dev",
        "build:stats": "webpack --profile --json > webpack-stats.json",
        "build:watch": "webpack --watch",
        "build:dev": "webpack",
        "build": "NODE_ENV=production webpack -p",
        "deploy": "npm --prefix apps/SiteBundle run build && git add -A && git commit -nm ':rocket: deploy' && git push && git subtree push --prefix apps/SiteBundle/web origin gh-pages"
    },
    "dependencies": {
        "bootstrap": "^4.1.0"
    },
    "devDependencies": {
        "babel-core": "^6.25.0",
        "babel-eslint": "^8.2.3",
        "babel-loader": "^7.1.4",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "brotli-gzip-webpack-plugin": "^0.5.0",
        "brotli-webpack-plugin": "^0.5.0",
        "browserslist": "^3.2.4",
        "css-loader": "^0.28.11",
        "eslint": "^4.19.1",
        "eslint-plugin-compat": "^2.2.0",
        "eslint-plugin-react": "^7.7.0",
        "expose-loader": "^0.7.5",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.11",
        "glob-all": "^3.1.0",
        "image-webpack-loader": "^4.2.0",
        "node-sass": "^4.8.3",
        "postcss-loader": "^2.1.3",
        "prop-types": "^15.6.1",
        "purify-css": "^1.2.5",
        "purifycss-webpack": "^0.7.0",
        "sass-loader": "^7.0.1",
        "style-loader": "^0.20.3",
        "url-loader": "^1.0.1",
        "webpack": "3.8.1"
    }
}
